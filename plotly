import plotly.graph_objects as go
from graph import data
import json
vals=json.loads(data)

sectors = [entry["sector"] for entry in vals]
impacts = [entry["impact"] for entry in vals]

fig = go.Figure(go.Bar(x=sectors, y=impacts))

fig.update_layout(
    title="Sample Sector Impacts",
    xaxis_title="Sector",
    yaxis_title="impacts"
)
fig.show()